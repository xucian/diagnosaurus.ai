# Diagnosaurus.ai Environment Configuration
# Copy this file to .env and fill in your API keys

# Anthropic API (Required)
ANTHROPIC_API_KEY=sk-ant-api03-...

# Skyflow API (Required for data sanitization)
SKYFLOW_VAULT_ID=your_vault_id_here
SKYFLOW_VAULT_URL=https://your-vault.vault.skyflowapis.com
SKYFLOW_API_KEY=your_skyflow_api_key_here

# Parallel.ai API (Required for medical research + clinic search)
PARALLEL_AI_API_KEY=your_parallel_ai_key_here

# Redis Configuration (MCP Server)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# RedisVL Configuration (Semantic Caching)
REDISVL_INDEX_NAME=diagnosaurus_cache
REDISVL_SIMILARITY_THRESHOLD=0.85

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=your_secret_key_here_change_in_production
PORT=5000

# MCP Server Configuration
MCP_REDIS_ENABLED=true
MCP_PARALLEL_ENABLED=true

# Application Settings (Optional - defaults in config.py)
# MAX_CONDITIONS=5
# AGENTS_BATCH=2
# CONFIDENCE_THRESHOLD=0.50
# MIN_PROBABILITY=0.05
# MAX_CLINICS=5

# Logging
LOG_LEVEL=INFO
LOG_FILE=diagnosaurus.log
